{
  "name": "waves-backend",
  "version": "1.0.0",
  "description": "Waves marine navigation backend service with PostGIS geospatial processing",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "ts-node database/migrate.ts",
    "db:seed": "ts-node database/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "marine",
    "navigation",
    "geospatial",
    "postgis",
    "depth-data",
    "boating",
    "safety"
  ],
  "author": "Waves Team",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/redis": "^6.1.1",
    "@fastify/websocket": "^8.3.1",
    "@fastify/static": "^6.12.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}