{
  "name": "waves-marine-navigation",
  "version": "1.0.0",
  "description": "Marine navigation platform with crowdsourced depth data and real-time safety features",
  "main": "index.js",
  "scripts": {
    "dev": "expo start",
    "deploy": "node deploy-marine-platform.js",
    "deploy:friends": "node deploy-friends-family.js",
    "deploy:friends-dry": "node deploy-friends-family.js --dry-run",
    "deploy:dev": "node deploy-marine-platform.js --env=dev --validate",
    "deploy:staging": "node deploy-marine-platform.js --env=staging --validate --dry-run",
    "deploy:production": "node deploy-marine-platform.js --env=production --validate",
    "test": "jest",
    "test:marine": "jest --testPathPattern=marine",
    "test:safety": "jest --testPathPattern=safety",
    "test:depth-validation": "jest --testPathPattern=depth.*validation",
    "test:navigation-safety": "jest --testPathPattern=navigation.*safety",
    "test:emergency-procedures": "jest --testPathPattern=emergency",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "build:lambda": "npm run build && cd backend && npx serverless package",
    "validate-deployment": "node deploy-marine-platform.js --dry-run --validate",
    "setup-db": "psql -f backend/migrations/001_initial_schema.sql",
    "load-test-data": "node backend/database/scripts/load_test_data.js --validate",
    "start:backend": "cd backend && npm run dev",
    "start:mobile": "cd WavesDemo && expo start",
    "docs:generate": "npx typedoc src/ --out docs/",
    "docs:serve": "npx http-server docs/ -p 8080",
    "agents": "node src/agents/cli/orchestrator-cli.js"
  },
  "keywords": [
    "marine",
    "navigation",
    "boat",
    "depth",
    "safety",
    "crowdsourced",
    "gps",
    "noaa",
    "react-native",
    "aws",
    "postgis"
  ],
  "author": "Seawater Marine Technologies",
  "license": "MIT",
  "dependencies": {
    "@aws-amplify/auth": "^6.0.0",
    "@aws-amplify/core": "^6.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@reduxjs/toolkit": "^2.0.0",
    "commander": "^14.0.0",
    "expo": "~50.0.0",
    "expo-location": "~16.5.0",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-redux": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "http-server": "^14.1.0",
    "jest": "^29.7.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1500.0",
    "serverless": "^3.38.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seawater-marine/waves-navigation.git"
  },
  "bugs": {
    "url": "https://github.com/seawater-marine/waves-navigation/issues"
  },
  "homepage": "https://waves.seawater.io"
}
